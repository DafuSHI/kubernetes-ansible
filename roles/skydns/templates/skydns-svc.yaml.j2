# From https://github.com/kubernetes/kubernetes/blob/master/cluster/libvirt-coreos/skydns-svc.yaml 

apiVersion: v1
kind: Service
metadata:
  labels:
    k8s-app: kube-dns
    kubernetes.io/cluster-service: "true"
  name: kube-dns
  namespace: kube-system
spec:
  portalIP: {{ dns_server }}
  ports:
  - port: 53
    protocol: UDP
    targetPort: 53
  selector:
    k8s-app: kube-dns

